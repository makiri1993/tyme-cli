{
    "name": "timesheet",
    "version": "0.0.0",
    "license": "MIT",
    "main": "./dist/index.js",
    "bin": "./dist/index.js",
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "dev": "TS_NODE_FILES=true NODE_ENV=development ts-node ./src",
        "build": "rm -rf dist && tsc && copyfiles --up 1 \"src/**/*.hbs\" dist/src/",
        "test": "jest --testPathIgnorePatterns 'tests/e2e/.*' 'example'",
        "e2e": "make e2e",
        "update": "make update",
        "lint": "eslint './src/**/*.{ts,tsx}'",
        "release": "yarn run build && np --any-branch",
        "release:pre": "yarn run build && np prerelease --any-branch --tag next --no-release-draft"
    },
    "files": [
        "dist/**/*",
        "src/cli.ts",
        "src/ui.ts"
    ],
    "dependencies": {
        "@babel/preset-react": "^7.8.0",
        "@babel/register": "^7.8.0",
        "@react-pdf/renderer": "^1.6.8",
        "@types/ink": "^2.0.3",
        "@types/node": "^13.1.7",
        "@types/react": "^16.9.17",
        "ava": "^2.4.0",
        "chalk": "^3.0.0",
        "eslint-config-xo-react": "^0.22.0",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "import-jsx": "^3.1.0",
        "ink": "^2.6.0",
        "ink-testing-library": "^1.0.3",
        "meow": "^6.0.0",
        "prettier": "^1.18.2",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "ts-node": "^8.6.2",
        "tslint": "^5.17.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^4.0.0",
        "typescript": "^3.7.4",
        "weeknumber": "^1.1.2",
        "xo": "^0.25.3"
    },
    "devDependencies": {},
    "ava": {
        "require": [
            "@babel/register"
        ]
    },
    "babel": {
        "presets": [
            "@babel/preset-react"
        ]
    },
    "xo": {
        "extends": "xo-react"
    }
}
